BEGIN:VEVENT
LOCATION:<%= ical_event.location.name %>, <%= ical_event.location.address %>
DTSTAMP:<%= ical_event.created_at.utc.strftime('%Y%m%dT%H%M%SZ') %>
URL;VALUE=URI:<%= url_for :action => 'show', :id => ical_event.id, :only_path => false %>
DTSTART;TZID=US/Pacific:<%= ical_event.starts_at.strftime('%Y%m%dT%H%M%S') %>
SUMMARY:<%= ical_event.name %>
DTEND;TZID=US/Pacific:<%= ical_event.ends_at.strftime('%Y%m%dT%H%M%S') %>
DESCRIPTION:<%= ical_event.agenda.split(/\r?\n/).join("\\n") %>
END:VEVENT

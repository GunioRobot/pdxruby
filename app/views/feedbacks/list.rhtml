<h1>Listing feedbacks</h1>

<table>
  <tr>
  <% for column in Feedback.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>

<% for feedback in @feedbacks %>
  <tr>
  <% for column in Feedback.content_columns %>
    <td><%=h feedback.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => feedback %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => feedback %></td>
    <td><%= link_to 'Destroy', {:action => 'destroy', :id => feedback}, :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<%= will_paginate @feedbacks %>

<br />

<%= link_to 'New feedback', :action => 'new' %>
